{
  "family": "documentportaltd",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::171158266231:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "document-portal-container",
      "image": "171158266231.dkr.ecr.us-east-1.amazonaws.com/documentportalliveclass",
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8052,
          "hostPort": 8052,
          "protocol": "tcp",
          "name": "document-portal-container-8052-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "API_KEYS",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:171158266231:secret:apikeyliveclass-qPHtGp"
        }
        
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}